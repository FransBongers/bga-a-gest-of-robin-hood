{
  "name": "agestofrobinhood",
  "version": "1.0.0",
  "description": "A Gest of Robin Hood for BGA",
  "main": "agestofrobinhood.js",
  "scripts": {
    "build:ts": "tsc",
    "build:scss": "sass --no-source-map src/agestofrobinhood.scss agestofrobinhood.css",
    "watch:ts": "tsc --watch",
    "watch:scss": "sass --watch src/agestofrobinhood.scss agestofrobinhood.css",
    "watch": "npm run watch:ts && npm run watch:scss",
    "build": "npm run build:ts && npm run build:scss"
  },
  "author": "Frans Bongers",
  "license": "MIT",
  "dependencies": {
    "bga-animations": "thoun/bga-animations#1.1.1",
    "bga-cards": "thoun/bga-cards#3.1.1"
  },
  "devDependencies": {
    "sass": "^1.77.2",
    "typescript": "^5.4.5"
  }
}
